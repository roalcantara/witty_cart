class WittyCart.Env
  constructor: ->
    @$env = '<%= WittyCart.env %>'
    @$woopraDomain = '<%= WittyCart.woopra_domain %>'
    @$tracking = <%= WittyCart.tracking? %>

  production: () ->
    @$env == 'production'

  woopraDomain: () ->
    @$woopraDomain

  tracking: () ->
    @$tracking

$(document).on 'turbolinks:load', ->
  window.WittyCart.env = new WittyCart.Env()
